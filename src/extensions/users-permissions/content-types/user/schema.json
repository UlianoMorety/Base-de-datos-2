{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "phone": {
      "type": "string"
    },
    "user_type": {
      "type": "enumeration",
      "enum": [
        "Estudiante",
        "Profesional",
        "Mentor",
        "Empresa"
      ],
      "required": true,
      "default": "Estudiante"
    },
    "cv": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "users_permissions_user"
    },
    "proyects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::proyect.proyect",
      "mappedBy": "users_permissions_user"
    },
    "internships": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::internship.internship",
      "mappedBy": "users_permissions_user"
    },
    "mentorings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::mentoring.mentoring",
      "mappedBy": "users_permissions_users"
    },
    "university": {
      "type": "string"
    },
    "studies": {
      "type": "string"
    }
  }
}
